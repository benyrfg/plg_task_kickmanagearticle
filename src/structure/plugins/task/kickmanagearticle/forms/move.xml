<?xml version="1.0" encoding="utf-8" ?>
<form>
	<fields name="params">
		<fieldset name="task_params">
	
			<field
				name="fromCatid"
				type="categoryedit"
				extension="com_content"
				label="PLG_TASK_KICKMANAGEARTICLE_FROM_CATEGORY_LABEL"
				addfieldprefix="Joomla\Component\Categories\Administrator\Field"
				required="true"
				default=""
			/>
			<field
				name="toCatid"
				type="categoryedit"
				extension="com_content"
				label="PLG_TASK_KICKMANAGEARTICLE_TO_CATEGORY_LABEL"
				addfieldprefix="Joomla\Component\Categories\Administrator\Field"
				required="true"
				default=""
			/>
			<field
				name="customField"
				type="sql"
				extension="com_content"
				label="PLG_TASK_KICKMANAGEARTICLE_SELECT_CF"
				required="true"
				default=""
				query="SELECT id, title FROM #__fields WHERE state=1"
				key_field="id"
				value_field="title"
			/>
		</fieldset>
	</fields>
</form>
 